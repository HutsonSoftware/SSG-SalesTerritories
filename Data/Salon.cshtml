@{
    var db = Database.Open("SSG-SalesTerritories");
    var salonID = Request["SalonID"];
    var sql = @"SELECT Name, Address, City, State, Zip, Latitude, Longitude, SalesRepID, SixMonthSales FROM Salon WHERE SalonID = @0";
    var data = db.Query(sql, salonID);
    Json.Write(data, Response.Output);
}