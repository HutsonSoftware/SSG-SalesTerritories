@{
    var db = Database.Open("SSG-SalesTerritories");
    var encodedPath = Request["EncodedPath"];

    var insertQuery = "INSERT INTO Territory (EncodedPath) VALUES (@0)";
    db.Execute(insertQuery, encodedPath);

    var identityQuery = @"SELECT @@IDENTITY FROM Territory;";
    var data = db.Query(identityQuery);
    Json.Write(data, Response.Output);
}